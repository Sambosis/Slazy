module slider() {
    union() {
        translate([0, 0, track_depth / 2])
        cube([slider_width, slider_depth, track_depth - 2 * guide_rail_dim]);

        translate([0, slider_depth / 2 - guide_rail_dim / 2, 0])
        cube([slider_width, guide_rail_dim, slider_depth]);
        
        translate([0, -slider_depth / 2 + guide_rail_dim / 2, 0])
        cube([slider_width, guide_rail_dim, slider_depth]);
    }
}

