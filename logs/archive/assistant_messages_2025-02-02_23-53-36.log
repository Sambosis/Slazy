        hull() {
            translate([-track_width/2, -track_depth/2, 0])
            cube([track_width/4, track_depth/4, track_length]);
            translate([-track_width/2, track_depth/2, 0])
            cube([track_width/4, track_depth/4, track_length]);
            translate([track_width/2, -track_depth/2, 0])
