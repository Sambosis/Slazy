    flex-direction: column;
    gap: 15px;
}

.option {
    background-color: #2196F3;
    color: white;
    padding: 12px 20px;
    font-size: 1.1em;
    border: none;
    border-radius: 8px;
    cursor: pointer;
    transition: background-color 0.3s ease, transform 0.2s ease;
}

.option:hover {
    background-color: #1976D2;
